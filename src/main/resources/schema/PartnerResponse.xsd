<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSpy v2016 rel. 2 (x64) (http://www.altova.com) by Douglas Huntzinger (Eli Lilly and Company) --><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">    <xs:element name="PartnerResponse">        <xs:annotation>            <xs:documentation>                The Root Element for responses used in Partner Response Messaging. Which can contain one or more Status objects of Success or Failure element.                attribute:SourceType - The source type i.e. Study, Site, etc.            </xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:sequence>                <xs:element ref="Status" minOccurs="0" maxOccurs="unbounded"/>            </xs:sequence>        </xs:complexType>    </xs:element>    <xs:element name="Status">        <xs:complexType>            <xs:choice>                <xs:element ref="Success"/>                <xs:element ref="Failure"/>            </xs:choice>            <xs:attribute name="SourceType" type="xs:string" use="required"/>        </xs:complexType>    </xs:element>    <xs:element name="Success">        <xs:annotation>            <xs:documentation>                The Success element is used to transfer aliasing information from the target system                to the source system. Attributes included within this element include:                attribute:OID - The source systems Object identifier	                attribute:TargetOID - The target systems aliasing information            </xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="OID" type="xs:string" use="required"/>            <xs:attribute name="TargetOID" type="xs:string" use="optional"/>        </xs:complexType>    </xs:element>    <xs:element name="Failure">        <xs:annotation>            <xs:documentation>                This Failure element is used to transfer errors found during processing of the                transaction data. Attributes included within this element include:                attribute:OID - The source systems Object identifier                attribute:Type - Failure type is limited to"Error" or "Warning"                attribute:Code - Error/Warning code                attribute:Description - A textual discription of the Error/Warning            </xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="OID" type="xs:string" use="optional"/>            <xs:attribute name="Type" type="FailureType" use="required"/>            <xs:attribute name="Code" type="xs:string" use="optional"/>            <xs:attribute name="Description" type="xs:string" use="required"/>        </xs:complexType>    </xs:element>    <xs:simpleType name="FailureType">        <xs:restriction base="xs:string">            <xs:enumeration value="Error"/>            <xs:enumeration value="Warning"/>        </xs:restriction>    </xs:simpleType></xs:schema>